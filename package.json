{
  "name": "react-study",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "npm run lint:es && npm run lint:style && npm run type-check",
    "lint:es": "eslint src/ --fix",
    "lint:es:check": "eslint src/ --max-warnings 0",
    "lint:style": "stylelint --fix \"src/**/*.{css,scss}\"",
    "lint:style:check": "stylelint \"src/**/*.{css,scss}\" --max-warnings 0",
    "format": "npm run format:prettier && npm run format:lint",
    "format:prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md,yml,yaml}\"",
    "format:lint": "npm run lint:es && npm run lint:style",
    "check-all": "npm run type-check && npm run lint:es:check && npm run lint:style:check && npm run format:check",
    "fix-all": "npm run format && npm run lint",
    "quality-check": "bash ./script/quality-check.sh"
  },
  "dependencies": {
    "@ant-design/colors": "^7.2.1",
    "@atlaskit/pragmatic-drag-and-drop": "^1.7.4",
    "@better-scroll/core": "^2.5.1",
    "@ctrl/tinycolor": "^4.1.0",
    "@react-spring/web": "^10.0.1",
    "@swiftcarrot/color-fns": "^3.2.0",
    "@use-gesture/react": "^10.3.1",
    "antd": "^5.26.3",
    "axios": "^1.10.0",
    "classnames": "^2.5.1",
    "cropperjs": "^2.0.0",
    "dayjs": "^1.11.13",
    "moment": "^2.30.1",
    "react": "^19.1.0",
    "react-cropper": "^2.3.3",
    "react-dom": "^19.1.0",
    "react-error-boundaries": "^1.1.4",
    "react-icons": "^5.5.0",
    "react-router": "^7.6.3",
    "react-transition-group": "^4.4.5",
    "spark-md5": "^3.0.2",
    "styled-components": "^6.1.19",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "@prettier/plugin-oxc": "^0.0.4",
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "^22.15.34",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-transition-group": "^4.4.12",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "globals": "^16.2.0",
    "lint-staged": "^16.1.2",
    "oxlint": "^1.4.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "sass": "^1.89.2",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "typescript-plugin-css-modules": "^5.1.0",
    "vite": "^6.3.5"
  }
}
